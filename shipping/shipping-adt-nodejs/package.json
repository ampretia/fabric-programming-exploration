{
    "name": "shipping-adt",
    "version": "0.0.1",
    "description": "My Smart Contract",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "pretest": "npm run lint",
        "test": "nyc mocha -r ts-node/register src/**/*.spec.ts",
        "start": "fabric-chaincode-node start",
        "metadata": "fabric-chaincode-node metadata generate --file metadata.json",
        "build": "tsc",
        "build:watch": "tsc -w",
        "prepublishOnly": "npm run build"
    },
    "engineStrict": true,
    "author": "John Doe",
    "license": "Apache-2.0",
    "dependencies": {
        "class-transformer": "^0.2.0",
        "fabric-contract-api": "^1.4.2-snapshot.28",
        "fabric-data": "file:fabric-data-0.0.1.tgz"
    },
    "peerDependencies": {
        "fabric-contract-api": "unstable-1.4",
        "fabric-shim": "unstable-1.4"
    },
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.30",
        "@types/sinon": "^5.0.7",
        "@types/sinon-chai": "^3.2.1",
        "chai": "^4.2.0",
        "fabric-client": "^1.4.1",
        "fabric-network": "^1.4.1",
        "mocha": "^5.2.0",
        "nyc": "^13.1.0",
        "sinon": "^7.2.7",
        "sinon-chai": "^3.3.0",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "typescript": "^3.4.5"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "coverage/**",
            "dist/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
